{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vmName": {
      "value": "<VM_NAME>"
    },
    "vmSize": {
      "value": "<VM_SIZE>"
    },
    "ubuntuOSVersion": {
      "value": "14.04.2-LTS"
    },
    "virtualNetworkName": {
      "value": "<VIRTUAL_NETWORK_NAME>"
    },
    "adminUsername": {
      "value": "<ADMIN_USER_NAME>"
    },
    "sshKeyData": {
      "value": "<SSH_KEY>"
    },
    "storageAccountName": {
      "value": "<STORAGE_ACCOUNT_NAME>"
    },
    "storageAccountKey": {
      "value": "<STORAGE_ACCOUNT_KEY>"
    },
    "fileUris": {
      "value": "[<SCRIPT1_URI> <SCRIPT2_URI> ...]"
    },
    "commandToExecute": {
      "value": "./script1.sh"
    }
  }
}
